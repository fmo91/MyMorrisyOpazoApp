  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Realm Test App
  pod 'R.swift'
  pod 'RealmSwift'

  target 'Realm Test AppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Realm Test AppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
